local AnimAnim = Instance.new("Animation")

AnimAnim.AnimationId = "rbxassetid://12296882427" -- the specific anim

local Anim = Humanoid:LoadAnimation(AnimAnim)


local startTime = 0.2 -- speed for specific


Anim:Play()

Anim:AdjustSpeed(0)

Anim.TimePosition = startTime

Anim:AdjustSpeed(0.8)

-- Create the ball part
local part = Instance.new("Part")
part.Shape = Enum.PartType.Ball
part.Size = Vector3.new(1, 1, 1)  -- Adjust the size of the ball as needed
part.Material = Enum.Material.Metal
part.Anchored = false  -- Ball shouldn't be anchored
part.CanCollide = false  -- Disable collision with other parts
part.Name = "Ball"
part.Parent = game.Workspace

-- Function to attach the ball to the right hand and move it forward
local function attachBallToHand(player)
    local character = player.Character or player.CharacterAdded:Wait()
    local rightHand = character:WaitForChild("RightHand")

    -- Attach the ball to the right hand using a Motor6D
    local motor = Instance.new("Motor6D")
    motor.Parent = rightHand
    motor.Part0 = rightHand
    motor.Part1 = ball
    motor.C0 = CFrame.new(1, 0, 0)  -- Adjust the position relative to the hand

    -- Keep the ball in the same position as the right hand
    ball.CFrame = rightHand.CFrame * CFrame.new(1, 0, 0)  -- Adjust this as necessary

    -- Move the ball forward after 1 second
    wait(1)
    local direction = rightHand.CFrame.LookVector  -- Direction the hand is facing
    local distance = 10  -- How far the ball will move
    ball.CFrame = ball.CFrame + direction * distance  -- Move the ball forward
end

-- Listen for when the player joins and attach the ball
game.Players.PlayerAdded:Connect(function(player)
    player.CharacterAdded:Connect(function()
        attachBallToHand(player)
    end)
end)

    wait(0.05)

local final1 = game.ReplicatedStorage.Resources.FSSlash.Attachment:Clone()
final1.Parent = game.Players.LocalPlayer.Character["Right Arm"]

for _, child in ipairs(final1:GetChildren()) do
    if child:IsA("ParticleEmitter") then -- Check if the child is a ParticleEmitter
        child:Emit(50) -- Emit 50 particles
    end
end

